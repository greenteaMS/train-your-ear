<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" prefWidth="300.0" spacing="5.0" stylesheets="@../resources/application.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="90.0" prefWidth="200.0" spacing="15.0">
         <children>
            <Button fx:id="firstLargerButton" maxHeight="70.0" maxWidth="70.0" minHeight="70.0" minWidth="70.0" mnemonicParsing="false" onAction="#firstLargerAnswer" prefHeight="70.0" prefWidth="70.0" styleClass="firstLargerButton" stylesheets="@../resources/buttons.css" text="" />
            <Button fx:id="theSameButton" maxHeight="70.0" maxWidth="70.0" minHeight="70.0" minWidth="70.0" mnemonicParsing="false" onAction="#theSameAnswer" prefHeight="70.0" prefWidth="70.0" styleClass="theSameButton" stylesheets="@../resources/buttons.css" text="" />
            <Button fx:id="secondLargerButton" maxHeight="70.0" maxWidth="70.0" minHeight="70.0" minWidth="70.0" mnemonicParsing="false" onAction="#secondLargerAnswer" prefHeight="70.0" prefWidth="70.0" styleClass="secondLargerButton" stylesheets="@../resources/buttons.css" text="" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="23.0">
         <children>
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#play" prefWidth="110.0" styleClass="tunerButton" stylesheets="@../resources/buttons.css" text="Graj" />
            <Button fx:id="showAnswer" mnemonicParsing="false" onAction="#showAnswer" prefWidth="110.0" styleClass="tunerButton" stylesheets="@../resources/buttons.css" text="Pokaż odpowiedź" />
         </children>
      </HBox>
      <Label fx:id="statusLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="200.0" />
      <Label fx:id="ratioValue" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="200.0" text="" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <styleClass>
      <String fx:value="settingsPaneWithBackground" />
      <String fx:value="comparatorPane" />
   </styleClass>
</fx:root>
