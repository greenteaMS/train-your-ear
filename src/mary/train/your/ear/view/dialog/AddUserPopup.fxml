<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<VBox prefHeight="120.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mary.train.your.ear.controller.dialog.AddUserPopupController">

	<children>
		<TextField fx:id="userName" maxWidth="350.0" minWidth="150.0" prefWidth="150.0" promptText="Nazwa użytkownika">
			<VBox.margin>
				<Insets left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</TextField>
		<Label fx:id="statusLabel" alignment="CENTER">
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
		</Label>
		<HBox alignment="CENTER" spacing="40.0">
			<children>
				<Button fx:id="addUserButton" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#addUser" styleClass="addChordButton" stylesheets="@../resources/buttons.css" text="Utwórz" textAlignment="CENTER" />
				<Button mnemonicParsing="false" onAction="#closeWindow" styleClass="addChordButton" stylesheets="@../resources/buttons.css" text="Zamknij" />
			</children>
			<VBox.margin>
				<Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
			</VBox.margin>
		</HBox>
	</children>
	<padding>
		<Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
	</padding>
</VBox>
