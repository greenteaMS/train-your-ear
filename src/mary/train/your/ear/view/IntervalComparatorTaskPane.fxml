			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
			<styleClass>
				<String fx:value="settingsPaneWithBackground" />
				<String fx:value="taskInfoPopupPanel" />
			</styleClass>
		</HBox>
		<TilePane fx:id="slidersList" alignment="CENTER" hgap="5.0" prefHeight="200.0" prefWidth="200.0" vgap="5.0" />

	</children>
