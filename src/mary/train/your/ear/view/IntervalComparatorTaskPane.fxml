<VBox fx:id="intervalComparatorTaskPane" alignment="TOP_CENTER" prefHeight="400.0" styleClass="settingsPaneWithBackground" stylesheets="@resources/application.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mary.train.your.ear.controller.tasks.IntervalComparatorTaskPaneController">
