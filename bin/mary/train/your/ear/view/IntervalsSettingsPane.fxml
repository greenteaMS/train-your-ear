<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="intervalsSettingsPane" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mary.train.your.ear.controller.settings.IntervalsSettingsPaneController">
   <children>
      <Label text="Available intervals">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <ScrollPane VBox.vgrow="SOMETIMES">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <content>
      
            <VBox fx:id="listOfButtons" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="190.0" spacing="5.0" />
         </content>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </ScrollPane>
      <CheckBox fx:id="simpleIntervals" mnemonicParsing="false" onAction="#setSimpleSetting" prefWidth="100.0" selected="true" text="Simple">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </CheckBox>
      <CheckBox fx:id="complexIntervals" mnemonicParsing="false" onAction="#setComplexSetting" prefWidth="100.0" text="Compound">
         <VBox.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" />
         </VBox.margin>
      </CheckBox>
   </children>
</VBox>
