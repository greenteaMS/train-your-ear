<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>


<fx:root alignment="TOP_CENTER" spacing="5.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="itemInfo" text="Label" />
      <Slider fx:id="frequencySlider" blockIncrement="1.0" majorTickUnit="10.0" max="20.0" min="-20.0" minorTickCount="5" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" />
      <Label fx:id="sliderVlalue" text="Hz">
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="playButton" mnemonicParsing="false" text="Play" />
            <Button fx:id="checkButton" mnemonicParsing="false" text="Check" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="result" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
